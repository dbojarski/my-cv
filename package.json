  "prettier": {
    "tabWidth": 2,
    "bracketSpacing": true,
    "trailingComma": "es5",
    "arrowParens": "always",
    "singleQuote": true,
    "jsxSingleQuote": true,
    "importOrder": [
      "<THIRD_PARTY_MODULES>",
      "^[./]"
